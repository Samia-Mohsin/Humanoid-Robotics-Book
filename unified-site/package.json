{
  "name": "physical-ai-humanoid-robotics-platform",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "concurrently --kill-others-on-fail \"npm run backend\" \"npm run frontend\"",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "frontend": "docusaurus start",
    "frontend:build": "docusaurus build",
    "frontend:swizzle": "docusaurus swizzle",
    "frontend:deploy": "docusaurus deploy",
    "frontend:serve": "docusaurus serve",
    "backend": "cd backend && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "backend:prod": "cd backend && gunicorn main:app -w 4 -k uvicorn.workers.UvicornWorker --bind 0.0.0.0:8000",
    "build": "npm run frontend:build",
    "clear": "docusaurus clear",
    "typecheck": "tsc",
    "setup": "npm install && cd backend && pip install -r requirements.txt",
    "migrate": "cd backend && python -m alembic upgrade head",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@docusaurus/core": "^3.1.0",
    "@docusaurus/preset-classic": "^3.1.0",
    "@docusaurus/theme-search-algolia": "^3.1.0",
    "@mdx-js/react": "^3.0.0",
    "better-auth": "^0.2.1",
    "clsx": "^2.0.0",
    "prism-react-renderer": "^2.3.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "^5.0.1"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^3.1.0",
    "@docusaurus/tsconfig": "^3.1.0",
    "@docusaurus/types": "^3.1.0",
    "concurrently": "^8.2.0",
    "typescript": "~5.2.2"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": "20.x"
  }
}